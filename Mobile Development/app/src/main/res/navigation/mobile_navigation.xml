<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/mobile_navigation"
    app:startDestination="@+id/navigation_home">

    <fragment
        android:id="@+id/navigation_home"
        android:name="com.example.terrestrial.ui.home.HomeFragment"
        android:label="@string/title_home"
        tools:layout="@layout/fragment_home" >
    </fragment>

    <fragment
        android:id="@+id/navigation_dashboard"
        android:name="com.example.terrestrial.ui.upload.UploadFragment"
        android:label="@string/title_upload_course"
        tools:layout="@layout/fragment_upload" />

    <fragment
        android:id="@+id/navigation_notifications"
        android:name="com.example.terrestrial.ui.setting.SettingFragment"
        android:label="@string/title_setting"
        tools:layout="@layout/settings_activity" >
    </fragment>
</navigation>